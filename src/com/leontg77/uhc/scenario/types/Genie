									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
						#Killstreak 4
						
						else if arg-2 is equal to "health":
							if {genie::%command sender%::kills} is equal to 4:
								if player has space for 1 wooden sword:							
									remove 1 from {genie::%command sender%::wishes}
									give player 1 potion of:8229
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"						

						else if arg-2 is equal to "ebottle":
							if {genie::%command sender%::kills} is equal to 4:
								if player has space for 128 bottles of enchanting:							
									remove 1 from {genie::%command sender%::wishes}
									give player 128 bottles of enchanting
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
						else if arg-2 is equal to "glowstone":
							if {genie::%command sender%::kills} is equal to 4:
								if player has space for 1 glowstone block:
									remove 1 from {genie::%command sender%::wishes}
									give player 1 glowstone block
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
						else if arg-2 is equal to "blazerod":
							if {genie::%command sender%::kills} is equal to 4:
								if player has space for 1 blaze rod:							
									remove 1 from {genie::%command sender%::wishes}
									give player 1 blaze rod
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
						#Killstreak 5+

						else if arg-2 is equal to "obsidian":
							if {genie::%command sender%::kills} is equal to 5:
								if player has space for 64 obsidian:
									remove 1 from {genie::%command sender%::wishes}
									give player 64 obsidian
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"						

						else if arg-2 is equal to "skull":
							if {genie::%command sender%::kills} is equal to 5:
								if player has space for 3 wither skeleton head:
									remove 1 from {genie::%command sender%::wishes}
									give player 3 wither skeleton head
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
						else if arg-2 is equal to "sand":
							if {genie::%command sender%::kills} is equal to 5:
								if player has space for 5 soul sand:					
									remove 1 from {genie::%command sender%::wishes}
									give player 5 soul sand
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
