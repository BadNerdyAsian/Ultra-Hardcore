									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
						else if arg-2 is equal to "gold":
							if {genie::%command sender%::kills} is equal to 5:
								if player has space for 8 gold ingot:
									remove 1 from {genie::%command sender%::wishes}
									give player 8 gold ingot
									message "&7------------------------------------------------"
									message "{@n} You now have &e%{genie::%command sender%::wishes}%&r wishes"
									message "&7------------------------------------------------"
									execute console command "/playsound note.harp %command sender% ~ ~ ~ 1 1 1"
								else:
									message "&7------------------------------------------------"
									message "{@n} &cError: You don't have room"
									message "&7------------------------------------------------"
									execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"										
							else:
								message "&7------------------------------------------------"
								message "{@n} &cError: Incorrect Killstreak &7- /genie wishes"
								message "&7------------------------------------------------"
								execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
						
						else:
							message "&7------------------------------------------------"
							message "{@n} &cError: Cannot find wish &7- /genie wishes"
							message "&7------------------------------------------------"
							execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
								
				else:
					message "&7------------------------------------------------"
					message "{@n} &cError: You have &e0&c wishes left"
					message "&7------------------------------------------------"
					execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
			else:
				message "&7------------------------------------------------"
				message "{@n} &cError: Genie is &lDisabled"
				message "&7------------------------------------------------"
				execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"			
					
		
		#/genie help
		else if arg-1 is equal to "help":
			message "&7------------------------------------------------"
			message "{@n} &eCommands:"
			if player has permission "{@perms}":
				message "{@n} /{@command} enable &7- Enables the Scenario"
				message "{@n} /{@command} disable &7- Disables the Scenario"
				message "{@n} /{@command} toggle &7- Toggles the Scenario"
			message "{@n} /{@command} wishes &7- How many wishes you have"
			message "{@n} /{@command} wish &7- Wish for a item here"
			message "{@n} /{@command} &7- Explains the Scenario"
			message "&7------------------------------------------------"
		
		#/genie awjoadpolwp
		else:
			message "&7------------------------------------------------"
			message "{@n} &cWrong Syntax - use /{@command} help"
			message "&7------------------------------------------------"
			execute console command "/playsound random.break %command sender% ~ ~ ~ 1 1 1"
			
#Event 'n' Sterf

on death of player:
	if {genie::isGoing} is set:
		delete {genie::%victim%::wishes}
		delete {genie::%victim%::kills}
		set {genie::%victim%::dead} to true
		if {genie::%attacker%::kills} is equal to 5:
			message "&7------------------------------------------------"
			message "{@n} Your killstreak &ecannot&r go any higher"
			message "&7------------------------------------------------"
			execute console command "/playsound random.break %attacker% ~ ~ ~ 1 1 1"
		else:
			add 1 to {genie::%attacker%::kills}
			message "&7------------------------------------------------"
			message "{@n} Your killstreak is now &e%{genie::%attacker%::kills}%"
			message "&7------------------------------------------------"
			execute console command "/playsound note.harp %attacker% ~ ~ ~ 1 1 1"
			
on join:
	if {genie::isGoing} is set:
		if {genie::%player%::wishes} is not set:
			if {genie::%player%::dead} is not set:
				set {genie::%player%::kills} to 0
				set {genie::%player%::wishes} to 3